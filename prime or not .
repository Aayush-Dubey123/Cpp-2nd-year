// Online C++ compiler to run C++ program online
#include <iostream>
int main()
{
int a,count=0;
    std::cout << "enter number = ";
    std::cin >>a;
    for(int i=1;i<=a;i++)
    {
    if (a%i==0)
    {
        count++;
    }
    }
    if(count==2)
    {
     std::cout << " a is prime " <<std::endl;
    }
      else
      {
        std::cout << " a is not prime " <<std::endl;
      }
     return 0;
}
